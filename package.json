{
	"publisher": "ayanamists",
	"name": "ayaseditor",
	"displayName": "AyaSEditor",
	"repository": {
		"type": "git",
		"url": "https://github.com/ayanamists/ayaseditor"
	},
	"description": "scheme, racket, lisp editor",
	"version": "0.1.5",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:scheme",
		"onLanguage:racket"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ayaseditor.Return",
				"title": "Return"
			},
			{
				"command": "ayaseditor.Tab",
				"title": "Tab"
			},
			{
				"command": "ayaseditor.Insert",
				"title": "Insert"
			}
		],
		"keybindings": [
			{
				"key": "Enter",
				"command": "ayaseditor.Return",
				"when": "editorTextFocus && !suggestWidgetVisible && editorLangId == scheme && (!vim.active || vim.mode == 'Insert')"
			},
			{
				"key": "Enter",
				"command": "ayaseditor.Return",
				"when": "editorTextFocus && !suggestWidgetVisible && editorLangId == racket && (!vim.active || vim.mode == 'Insert')"
			},
			{
				"key": "Enter",
				"command": "ayaseditor.Return",
				"when": "editorTextFocus && !suggestWidgetVisible && editorLangId == lisp && (!vim.active || vim.mode == 'Insert')"
			},
			{
				"key": "Tab",
				"command": "ayaseditor.Tab",
				"when": "editorTextFocus && editorLangId == scheme"
			},
			{
				"key": "Tab",
				"command": "ayaseditor.Tab",
				"when": "editorTextFocus && editorLangId == racket"
			},
			{
				"key": "Tab",
				"command": "ayaseditor.Tab",
				"when": "editorTextFocus && editorLangId == lisp"
			},
			{
				"key": "ctrl+\\",
				"command": "ayaseditor.Insert",
				"when": "editorTextFocus && (!vim.active || vim.mode == 'Insert')"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
